{
  "manifest_version": 2,
  "name": "rot13",
  "version": "1.0.0",

  "description": "rot13 for Twitter",

  "author": "@zip",

  "permissions": [
    "https://twitter.com/"
  ],

  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["stylesheets/style.css"],
      "js": ["js/content.js"]
    }
  ]
}
